<archetype xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0 http://maven.apache.org/xsd/archetype-1.0.0.xsd">
    <id>neo4j-maven-archetype</id>
    <allowPartial>true</allowPartial>
    <sources>
        <source>src/main/java/DummyClass.java</source>
    </sources>
    <resources>
        <resource>src/main/resources/DummyResource.txt</resource>
    </resources>
    <testSources>
        <source>src/test/java/DummyTestClass.java</source>
    </testSources>
    <testResources>
        <resource>src/test/resources/DummyTestResource.txt</resource>
    </testResources>
</archetype>